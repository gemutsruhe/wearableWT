<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/weightNumTextView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:padding="8dp"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/repeatTextView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:padding="8dp"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/setNumTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:padding="8dp"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/weightNumTextView1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:padding="8dp"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/repeatTextView1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:padding="8dp"
        android:textSize="16dp" />

    <ImageButton
        android:id="@+id/deleteSetButton"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="6dp"
        app:srcCompat="@drawable/delete_set"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:background="@android:color/transparent"/>

</LinearLayout>